{
  "name": "mylife",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "devstart": "cross-env NODE_ENV=dev nodemon ./bin/www",
    "migrate": "knex --env test migrate:latest",
    "migrate:reset": "knex --env test migrate:rollback && npm run migrate",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000 --detectOpenHandles",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset"
  },
  "dependencies": {
    "async": "^3.1.0",
    "bcryptjs": "^2.4.3",
    "connect-ensure-login": "^0.1.1",
    "connect-flash": "^0.1.1",
    "connect-session-knex": "^2.0.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.18.2",
    "express-bouncer": "^0.2.0",
    "express-session": "^1.17.0",
    "express-validator": "^6.2.0",
    "googleapis": "^39.2.0",
    "http-errors": "~1.6.3",
    "js-base64": "^2.5.2",
    "knex": "^0.20.1",
    "mailparser": "^2.7.7",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "node-email-reply-parser": "^0.1.1",
    "node-quill-converter": "^0.3.3",
    "node-sass-middleware": "0.11.0",
    "node-schedule": "^1.3.2",
    "objection": "^1.6.11",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.12.1",
    "sentencer": "^0.2.0",
    "sharp": "^0.25.4"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "faker": "^4.1.0",
    "jest": "^26.4.0",
    "nodemon": "^2.0.1",
    "supertest": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
